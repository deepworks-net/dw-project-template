{
  "full_name": "Deepworks",
  "email": "support@deepworks.net",
  "github_username": "deepworks-net",
  "project_name": "DeepWork Cookiecutter Project Template",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
  "project_short_description": "This cookiecutter template serves to create the boilerplate code for a new DeepWork project",
  "version": "0.1.0",
  "has_docker": true,
  "image_home_dir?{{cookiecutter.has_docker}}": "/.deepworks/",
  "image_init_command?{{cookiecutter.has_docker}}": "/sbin/init",
  "image_name?{{cookiecutter.has_docker}}": "deepworks/{{ cookiecutter.project_slug }}",
  "image_version?{{cookiecutter.has_docker}}": "0.0.1",
  "base_image_name?{{cookiecutter.has_docker}}": "alpine",
  "base_image_version?{{cookiecutter.has_docker}}": "3.15.0",
  "service_name?{{cookiecutter.has_docker}}": "latest",
  "open_source_license": ["MIT license", "BSD license", "ISC license", "Apache Software License 2.0", "GNU General Public License v3", "Not open source"]
